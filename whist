if not isfile("YcaOffice\\Success\\SynapseX.exp") then
	if syn then
		writefile("YcaOffice\\Success\\SynapseX.exp", "Synapse Detected")
		print("Synapse X")
		return
	end
end

if not isfile("YcaOffice\\Success\\KRNL.exp") then
	if KRNL_LOADED then
		writefile("YcaOffice\\Success\\KRNL.exp", "Krnl Detected")
		print("KRNL")
		return
	end
end

